[Transfer]
ProtectVersion=%A

[Source]
Type=url-file
Path=https://duranium.craftyguy.net/%M/
MatchPattern=%M_@v.efi

[Target]
Type=regular-file
Path=/EFI/Linux
PathRelativeTo=boot
MatchPattern=%M_@v+@l-@d.efi \
             %M_@v+@l.efi \
             %M_@v.efi
Mode=0600
TriesLeft=3
TriesDone=0
InstancesMax=2
